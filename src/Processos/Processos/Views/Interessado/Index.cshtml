@{
    ViewData["Title"] = "Listagem de Interessados";
}

@model IEnumerable<Processos.Models.Interessado>




<DIV id='formBusca'>
    <DIV><input type="text" id="Busca" class="form-control form-control-sm" placeholder="Procurar interessados cadastrados"></DIV>
    <DIV id="contenedorBuscaLista"></DIV>
</DIV>


<SCRIPT>

    $("#Busca").change(function () {
        lista("Interessado", "INTERESSADO")
    })

    $("#Busca").on("keypress", function (e) {
        if (e.which == 13) {
            lista("Interessado", "INTERESSADO")
        }
    })

    $(document).ready(function () {
        lista("Interessado", "INTERESSADO")
    });



</SCRIPT>
