@{
    ViewData["Title"] = "Listagem de Setores";
}

@model IEnumerable<Processos.Models.Setor>


<DIV id='formBusca'>
    <DIV><input type="text" id="Busca" class="form-control form-control-sm" placeholder="Procurar setores cadastrados"></DIV>
    <DIV id="contenedorBuscaLista"></DIV>
</DIV>


<SCRIPT>

    $("#Busca").change(function () {
        lista("Setor", "SETOR")
    })

    $("#Busca").on("keypress", function (e) {
        if (e.which == 13) {
            lista("Setor", "SETOR")
        }
    })

    $(document).ready(function () {
        lista("Setor", "SETOR")
    });



</SCRIPT>
