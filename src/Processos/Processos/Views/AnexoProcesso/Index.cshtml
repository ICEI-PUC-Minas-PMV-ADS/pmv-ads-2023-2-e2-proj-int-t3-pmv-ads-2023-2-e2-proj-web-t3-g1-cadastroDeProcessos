@model IEnumerable<Processos.Models.AnexoProcesso>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.codigoProcesso)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.dataHora)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.nomeAnexo)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.codigoProcesso)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.dataHora)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.nomeAnexo)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.codigoAnexo">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.codigoAnexo">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.codigoAnexo">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
