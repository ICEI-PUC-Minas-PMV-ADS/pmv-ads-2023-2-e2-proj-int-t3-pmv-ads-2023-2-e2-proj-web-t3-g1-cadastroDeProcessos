@model Processos.Models.Fluxo

@{
    ViewData["Title"] = "Apagar";
}



<h3>Você deseja excluir esse fluxo?</h3>
<div>
    <h4>Fluxo</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.codigoSetorOrigem)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.codigoSetorOrigem)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.codigoSetorDestino)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.codigoSetorDestino)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.codigoTipoProcesso)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.codigoTipoProcesso)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="codigoFluxo" />
        <input type="submit" value="Apagar" class="btn btn-danger" /> |
            <button class="btn btn-outline-secondary"><a asp-action="Index">Voltar</a></button>
    </form>
</div>
