@{
    ViewData["Title"] = "Listagem de Fluxo";
}

@model IEnumerable<Processos.Models.Fluxo>




<DIV id='formBusca'>
    <DIV><input type="text" id="Busca" class="form-control form-control-sm" placeholder="Procurar por fluxos cadastrados"></DIV>
    <DIV id="contenedorBuscaLista"></DIV>
</DIV>


<SCRIPT>

    $("#Busca").change(function () {
        lista("Fluxo", "FLUXO")
    })

    $("#Busca").on("keypress", function (e) {
        if (e.which == 13) {
            lista("Fluxo", "FLUXO")
        }
    })

    $(document).ready(function(){
        lista("Fluxo", "FLUXO")
    });

    

</SCRIPT>
