    <!-- 
        Speedbutton recebe uma janela com vínculo
    -->
    <DIV id="contenedorSpeedbutton">
        <DIV id="contenedorSpeedbuttontitulo">
            <span style="width:90%">Busca</span>
            <span style="float:right"><a href="javascript:fecharSpeedbutton()">[fechar]</a></span>
        </DIV>
    
        <DIV><input type="text" id="buscaSpeedButton"></DIV>
        <DIV id="speedbutton"></DIV>        
    </DIV>


    <SCRIPT>

        var ultimaURLSpeedButtonSemABusca = "";

        $("#buscaSpeedButton").change(function(){
                $("#speedbutton").load(ultimaURLSpeedButtonSemABusca + valor("buscaSpeedButton"));
        })

        $("#buscaSpeedButton").on("keypress", function (e) {
            if (e.which == 13) {
                $("#speedbutton").load(ultimaURLSpeedButtonSemABusca + valor("buscaSpeedButton"));
            }
        })

    </SCRIPT>



