@{
    ViewData["Title"] = "Listagem de Usuários";
}

@model IEnumerable<Processos.Models.Usuario>



<DIV id='formBusca'>
    <DIV><input type="text" id="Busca" class="form-control form-control-sm" placeholder="Procurar"></DIV>
    <DIV id="contenedorBuscaLista"></DIV>
</DIV>


<SCRIPT>

    $("#Busca").change(function () {
        lista("Usuario", "USUARIO")
    })

    $("#Busca").on("keypress", function (e) {
        if (e.which == 13) {
            lista("Usuario", "USUARIO")
        }
    })

    $(document).ready(function () {
        lista("Usuario", "USUARIO")
    });



</SCRIPT>
