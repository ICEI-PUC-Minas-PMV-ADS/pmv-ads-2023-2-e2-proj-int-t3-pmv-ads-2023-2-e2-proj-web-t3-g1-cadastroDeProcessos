@{
    ViewData["Title"] = "Listagem de Tipos de Processo";
}

@model IEnumerable<Processos.Models.Tipo_Processo>





<DIV id='formBusca'>
    <DIV><input type="text" id="Busca" class="form-control form-control-sm" placeholder="Procurar tipos de processo cadastrados"></DIV>
    <DIV id="contenedorBuscaLista"></DIV>
</DIV>


<SCRIPT>

    $("#Busca").change(function () {
        lista("Tipo_Processo", "TIPO_PROCESSO")
    })

    $("#Busca").on("keypress", function (e) {
        if (e.which == 13) {
            lista("Tipo_Processo", "TIPO_PROCESSO")
        }
    })

    $(document).ready(function () {
        lista("Tipo_Processo", "TIPO_PROCESSO")
    });


</SCRIPT>
