@model IEnumerable<Processos.Models.Processo>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.codigoInteressado)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.codigoTipoProcesso)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.cpfProtocolista)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.codigoSetor)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.dataHora)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.resumo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.assunto)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.codigoInteressado)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.codigoTipoProcesso)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.cpfProtocolista)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.codigoSetor)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.dataHora)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.resumo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.assunto)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.codigoProcesso">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.codigoProcesso">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.codigoProcesso">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
