@{
    ViewData["Title"] = "Listagem de Processos";
}

@model IEnumerable<Processos.Models.Processo>



<DIV id='formBusca'>
    <DIV><input type="text" id="Busca" class="form-control form-control-sm" placeholder="Procurar processos cadastrados"></DIV>
    <DIV id="contenedorBuscaLista"></DIV>
</DIV>


<SCRIPT>

    $("#Busca").change(function () {
        lista("Processo", "PROCESSO")
    })

    $("#Busca").on("keypress", function (e) {
        if (e.which == 13) {
            lista("Processo", "PROCESSO")
        }
    })

    $(document).ready(function () {
        lista("Processo", "PROCESSO")
    });



</SCRIPT>
